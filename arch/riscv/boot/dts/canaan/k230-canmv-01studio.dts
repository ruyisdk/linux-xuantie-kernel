#include "k230.dtsi"

/ {
	model = "Canaan CanMV-K230";
	compatible = "canaan,canmv-k230", "canaan,kendryte-k230";

	chosen {
		stdout-path = "serial0:115200n8";
	};

	ddr: memory@0 {
		device_type = "memory";
		reg = <0x0 0x0 0x0 0x40000000>;
	};
};

&uart0 {
	status = "okay";
};
&mmc_sd0{
	status = "okay";
	no-1-8-v;
	cap-sd-highspeed;
	rx_delay_line = <0x0d>;
	tx_delay_line = <0xb0>;
};
&mmc_sd1{
	status = "okay";
	no-1-8-v;
	cap-sd-highspeed;
	mshc_ctrl_r = <0x00>;
};
&usb0 {
	status = "okay";
};
&usb1 {
	dr_mode = "host";
	status = "okay";
};
